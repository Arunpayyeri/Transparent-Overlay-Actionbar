<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <!-- overlay actionbar....-->
    <style name="AppTheme.Transp">
        <item name="android:actionBarStyle">@style/MyActionBar1</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="actionBarStyle">@style/MyActionBar1</item>
        <item name="windowActionBarOverlay">true</item>

    </style>
    <color name="traa1">#00000000</color>

    <style name="MyActionBar1" parent="Widget.AppCompat.ActionBar.Solid">
    <item name="android:background">@color/traa1</item>
    <item name="background">@color/traa1</item>
    </style>
</resources>
